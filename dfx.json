{
  "canisters": {
    "helloApp": {
      "main": "src/helloApp/main.mo",
      "type": "motoko"
    },
    "helloApp_assets": {
      "dependencies": [
        "helloApp"
      ],
      "frontend": {
        "entrypoint": "src/helloApp_assets/src/index.html"
      },
      "source": [
        "src/helloApp_assets/assets",
        "dist/helloApp_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}